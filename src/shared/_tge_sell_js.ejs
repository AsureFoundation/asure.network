<script>
    function updateTgeContractDetails($details) {
        $details.find('.tge-contract-addr').text(tgeData.preSale.addr);
        $details.find('.tge-contract-addr').attr('href', 'https://etherscan.io/address/' + tgeData.preSale.addr);
    }

    function updateStartCounter($counter, startMoment) {
        function update() {
            var duration = moment.duration(startMoment.diff(moment()));

            $counter.find('.counter-days').text(duration.days());
            $counter.find('.counter-hours').text(duration.hours());
            $counter.find('.counter-minutes').text(duration.minutes());
            $counter.find('.counter-seconds').text(duration.seconds());
        }

        update();
        return setInterval(update, 1000);
    }


    function createPreSaleContract(provider) {
        // The Contract interface
        var abi = tgeContractsAbis.AsureCrowdsale;
        var address = tgeData.preSale.addr;

        return new ethers.Contract(address, abi, provider);
    }

    /**
     * See https://metamask.github.io/metamask-docs/Best_Practices/Registering_Your_Token
     * See https://github.com/estebanmino/EIPs/blob/master/EIPS/eip-747.md
     */
    function suggestTokenInMetaMask(provider) {
        return provider.send('metamask_watchAsset', {
            type: 'ERC20',
            options: {
                address: tgeData.token.addr,
                symbol: tgeData.token.symbol,
                decimals: tgeData.token.decimals,
                image: 'data:image/png;base64,'
            }
        }).then(function(res) {
            console.log('suggestTokenInMetaMask', res);
        }, function(err) {
            console.error('suggestTokenInMetaMask', err);
        });
    }

    $(function () {
        if (typeof window.ethereum === 'undefined') {
            console.warn('MetaMask not available.');
            return;
        }


        var opening = moment(tgeData.preSale.opening, tgeData.dateFormat);
        var closing = moment(tgeData.preSale.closing, tgeData.dateFormat);

        updateTgeContractDetails($('.card-tge-contract-details'));
        updateStartCounter($('.card-start-counter'), opening);


        let provider = new ethers.providers.Web3Provider(window.ethereum);

        provider.getNetwork().then(function (network) {
            console.log('Network', network);
        });

        //suggestTokenInMetaMask(provider);

        const preSale = createPreSaleContract(provider);
        preSale.isOpen().then(function(isOpen) {
            console.log('PreSale isOpen', isOpen);
        });
    });
</script>
